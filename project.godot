; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CrossProductVisualizer"
run/main_scene="res://Scene.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
boot_splash/bg_color=Color(0.27451, 0.258824, 0.247059, 1)
boot_splash/show_image=false
boot_splash/image="res://splashScreen.png"
boot_splash/fullsize=false
config/icon="res://ico.ico"
config/windows_native_icon="res://ico.ico"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720

[dotnet]

project/assembly_name="CrossProductVisualizer"

[rendering]

textures/vram_compression/import_etc2_astc=true
anti_aliasing/quality/msaa_3d=3
anti_aliasing/quality/screen_space_aa=1
